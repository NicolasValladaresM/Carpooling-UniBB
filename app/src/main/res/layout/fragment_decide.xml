<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="298dp"
    android:layout_height="600dp"
    android:padding="5dp"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".DecideFragment"
    android:background="@drawable/backgroudnpersonselect"
    android:layout_centerHorizontal="true"
    android:layout_alignParentTop="true"
    android:elevation="4dp"
    android:id="@+id/decideF">


    <View
        android:id="@+id/personicon"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:background="@mipmap/personazulicon"
        android:elevation="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />


    <TextView
        android:id="@+id/infoD"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/personicon"
        app:layout_constraintEnd_toEndOf="parent"
        android:textSize="20dp"
        android:text="Información de usuario"
        android:layout_marginBottom="350dp"
        android:textColor="@color/black"
        android:textStyle="bold"

        />


    <TextView
        android:id="@+id/inforepuD"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toTopOf="@id/iconpromeD"
        app:layout_constraintEnd_toEndOf="parent"
        android:textSize="20dp"
        android:text="Puntos de Reputación"
        android:layout_marginBottom="20dp"
        android:textColor="@color/black"
        android:textStyle="bold"

        />


    <View
        android:id="@+id/nameiconD"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:background="@mipmap/personainfotarjet"
        android:elevation="10dp"

        app:layout_constraintEnd_toStartOf="@id/nameD"
        app:layout_constraintTop_toTopOf="@id/nameD"
        app:layout_constraintBottom_toBottomOf="@+id/nameD"
        android:layout_marginRight="10dp"

        />

    <TextView
        android:id="@+id/nameD"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toTopOf="@id/btn_boton1D"
        android:textSize="16dp"
        android:text="....."
        android:layout_marginBottom="320dp"
        android:textColor="@color/black"
        android:layout_marginLeft="60dp"

        android:textStyle="bold"
        />







    <View
        android:id="@+id/carrericon"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:background="@mipmap/carrerapersonainfo"
        android:elevation="10dp"
        app:layout_constraintEnd_toStartOf="@id/carrerD"
        android:layout_marginRight="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/carrerD"
        app:layout_constraintTop_toTopOf="@id/carrerD"

        />


    <TextView
        android:id="@+id/carrerD"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toTopOf="@id/btn_boton1D"
        android:textSize="16dp"
        android:text="....."
        android:layout_marginBottom="260dp"
        android:textColor="@color/black"
        android:layout_marginLeft="60dp"
        android:textStyle="bold"
        />



    <View
        android:id="@+id/generoicon"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:background="@mipmap/generopersonatarget"
        android:elevation="10dp"
        app:layout_constraintEnd_toStartOf="@id/genderD"
        app:layout_constraintTop_toTopOf="@id/genderD"
        app:layout_constraintBottom_toBottomOf="@+id/genderD"
        android:layout_marginRight="10dp"


        />

    <TextView
        android:id="@+id/genderD"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toTopOf="@id/btn_boton1D"
        android:textSize="16dp"
        android:text="....."
        android:layout_marginBottom="290dp"
        android:textColor="@color/black"
        android:layout_marginLeft="60dp"
        android:textStyle="bold"


        />










    <View
        android:id="@+id/seaticonD"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:background="@mipmap/seaticon"
        android:elevation="10dp"

        app:layout_constraintEnd_toStartOf="@id/selectNumberD"

        android:layout_marginRight="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/selectNumberD"
        app:layout_constraintTop_toTopOf="@id/selectNumberD"
        android:visibility="invisible"

        />

    <TextView
        android:id="@+id/selectNumberD"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toTopOf="@id/btn_boton1D"
        android:layout_marginBottom="230dp"
        android:textSize="16dp"
        android:text=""
        android:textColor="@color/black"
        android:layout_marginLeft="60dp"
        android:textStyle="bold"

        />





    <View
        android:id="@+id/infoVehiculoViewD"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:background="@mipmap/autoinfo"
        android:elevation="10dp"
        app:layout_constraintEnd_toStartOf="@id/infoVehiculoPopD"
        android:layout_marginRight="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/infoVehiculoPopD"
        app:layout_constraintTop_toTopOf="@id/infoVehiculoPopD"
        android:visibility="invisible"
        />

    <TextView
        android:id="@+id/infoVehiculoPopD"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toTopOf="@id/btn_boton1D"
        android:layout_marginBottom="200dp"
        android:textSize="16dp"
        android:text=""
        android:textColor="@color/black"
        android:layout_marginLeft="60dp"
        android:textStyle="bold"

        />




    <View
        android:id="@+id/infoVehiculoViewPatD"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:background="@mipmap/patente"
        android:elevation="10dp"
        app:layout_constraintEnd_toStartOf="@id/infoVehiculoPatD"
        android:layout_marginRight="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/infoVehiculoPatD"
        app:layout_constraintTop_toTopOf="@id/infoVehiculoPatD"
        android:visibility="invisible"
        />

    <TextView
        android:id="@+id/infoVehiculoPatD"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toTopOf="@id/btn_boton1D"
        android:layout_marginBottom="170dp"
        android:textSize="16dp"
        android:text=""
        android:textColor="@color/black"
        android:layout_marginLeft="60dp"
        android:textStyle="bold"

        />






    <TextView
        android:id="@+id/MmaloD"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toTopOf="@id/btn_boton1D"
        android:layout_marginBottom="100dp"
        android:textSize="18dp"
        android:text=""
        android:textColor="@color/black"
        android:textStyle="bold"
        android:layout_marginLeft="30dp"



        />

    <TextView
        android:id="@+id/MaloD"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toTopOf="@id/btn_boton1D"
        android:layout_marginBottom="80dp"
        android:layout_marginLeft="30dp"

        android:textSize="18dp"
        android:text=""
        android:textColor="@color/black"
        android:textStyle="bold"

        />
    <TextView
        android:id="@+id/RegularD"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toTopOf="@id/btn_boton1D"
        android:layout_marginBottom="60dp"
        android:layout_marginLeft="30dp"

        android:textSize="18dp"
        android:text=""
        android:textColor="@color/black"
        android:textStyle="bold"

        />
    <TextView
        android:id="@+id/BuenoD"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toTopOf="@id/btn_boton1D"
        android:layout_marginBottom="40dp"
        android:layout_marginLeft="30dp"

        android:textSize="18dp"
        android:text=""
        android:textColor="@color/black"
        android:textStyle="bold"

        />

    <TextView
        android:id="@+id/MbuenoD"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toTopOf="@id/btn_boton1D"
        android:layout_marginBottom="20dp"
        android:textSize="18dp"
        android:text=""
        android:textColor="@color/black"
        android:textStyle="bold"
        android:layout_marginLeft="30dp"


        />




    <View
        android:id="@+id/iconpromeD"
        android:layout_width="95dp"
        android:layout_height="92dp"
        android:elevation="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toTopOf="@id/btn_boton1D"
        android:layout_marginBottom="20dp"
        android:layout_marginRight="40dp"
        />

    <Button
        android:id="@+id/btn_boton1D"
        android:layout_width="133dp"
        android:layout_height="60dp"
        android:layout_marginStart="16dp"
        android:text="Aceptar traslado"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:textColor="@color/white"
        android:textSize="16dp"

        android:layout_marginLeft="50dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="500dp"



        />



    <Button
        android:id="@+id/btn_boton2D"
        android:layout_width="133dp"
        android:layout_height="60dp"
        android:layout_marginStart="32dp"
        android:textSize="16dp"
        android:text="Rechazar Traslado"
        app:layout_constraintStart_toEndOf="@id/btn_boton1D"
        app:layout_constraintTop_toTopOf="parent"
        android:textColor="@color/white"
        android:backgroundTint="@color/red_block"
        android:layout_marginLeft="186dp"
        android:layout_marginRight="50dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="500dp"

        />

</androidx.constraintlayout.widget.ConstraintLayout>